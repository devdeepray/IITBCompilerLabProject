/* Test case for semantic checks */

/* function declaration errors */

int main(int a, float a)
{
	return a;
} 

int f(int a[3][5], int b)
{
	return b;
}

float f(int x[3][5], int y)
{
	return y;
}

float g(int a, int b)
{
	float a[3][5];
	int x,y,z[0];
	return a[1][2];
}

/* Expression errors */

void z()
{
	int x,y,z;
	float a,b,c;
	int p[100][300];
	
	x = ((a + b) * (p[100] - p[100][2]) / (a[1])) * 3;
	return x;
}



